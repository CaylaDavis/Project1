{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "TechETL"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzurePostgreSql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzurePostgreSql1'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"CosmosDbNoSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql1'"
		},
		"CosmosDbNoSql2TechCustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql2TechCustomer'"
		},
		"ProjectWarehouse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ProjectWarehouse'"
		},
		"AzurePostgreSql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "citus324.postgres.database.azure.com"
		},
		"AzurePostgreSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"AzurePostgreSql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "citus"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "citus111.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "GlobalTechWarehouse"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "citus"
		},
		"ProjectWarehouse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "citus331.database.windows.net"
		},
		"ProjectWarehouse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ProjectFinals"
		},
		"ProjectWarehouse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "citus"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "TechETL_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzurePostgreSql1_password": {
						"value": "[parameters('AzurePostgreSql1_password')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"CosmosDbNoSql1_connectionString": {
						"value": "[parameters('CosmosDbNoSql1_connectionString')]"
					},
					"CosmosDbNoSql2TechCustomer_connectionString": {
						"value": "[parameters('CosmosDbNoSql2TechCustomer_connectionString')]"
					},
					"ProjectWarehouse_password": {
						"value": "[parameters('ProjectWarehouse_password')]"
					},
					"AzurePostgreSql1_properties_typeProperties_server": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_server')]"
					},
					"AzurePostgreSql1_properties_typeProperties_database": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_database')]"
					},
					"AzurePostgreSql1_properties_typeProperties_username": {
						"value": "[parameters('AzurePostgreSql1_properties_typeProperties_username')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"ProjectWarehouse_properties_typeProperties_server": {
						"value": "[parameters('ProjectWarehouse_properties_typeProperties_server')]"
					},
					"ProjectWarehouse_properties_typeProperties_database": {
						"value": "[parameters('ProjectWarehouse_properties_typeProperties_database')]"
					},
					"ProjectWarehouse_properties_typeProperties_userName": {
						"value": "[parameters('ProjectWarehouse_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}